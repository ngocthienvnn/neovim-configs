{
   "languageserver": {
      "intelephense": {
         "command": "intelephense",
         "args": ["--stdio"],
         "filetypes": ["php"],
         "initializationOptions": {
            "storagePath": "/tmp/intelephense"
         },
         "settings": {
            "diagnostics.undefinedTypes": false,
            "diagnostics.undefinedFunctions": false,
            "diagnostics.undefinedConstants": false,
            "diagnostics.undefinedClassConstants": false,
            "diagnostics.undefinedMethods": false,
            "diagnostics.undefinedProperties": false
         }
      }
},
  "suggest.autoTrigger": "always",
  "suggest.noselect": false,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": false,
  "tsserver.npm": "/usr/local/bin/npm",
  "suggest.keepCompleteopt": false,
  "coc.source.buffer.enable": false,
  "coc.source.file.enable": false,
  "coc.source.around.enable": false,
  "diagnostic.refreshAfterSave": true,
  "suggest.maxCompleteItemCount": 10,
  "suggest.snippetIndicator": "â–º",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.colorSupport": false,
  "diagnostic.messageTarget": "echo",
  "suggest.disableKind": true,
  "suggest.floatEnable": false
}



